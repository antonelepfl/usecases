{
  "name": "bbp-usecase-wizard",
  "description": "Blue Brain Usecase Wizard",
  "version": "1.1.0",
  "private": true,
  "author": "Antonel Stefano <stefano.antonel@epfl.ch>",
  "contributors": [
    "michael.gevaert@epfl.ch",
    "stefano.antonel@epfl.ch",
    "jean-denis.courcol@epfl.ch",
    "michele.migliore@yale.edu"
  ],
  "license": "GPL",
  "repository": {
    "type": "git",
    "url": "https://github.com/antonelepfl/usecases/releases",
    "issuesurl": "https://github.com/antonelepfl/usecases/issues"
  },
  "scripts": {
    "dev": "vue-cli-service serve --port 8282",
    "build": "vue-cli-service build",
    "lint": "eslint src/assets/config_files/usecases.json && vue-cli-service lint",
    "build_doc": "jsdoc --readme ./README.md src/main.js --destination docs"
  },
  "dependencies": {
    "@sentry/browser": "^5.0.6",
    "@sentry/integrations": "^5.0.8",
    "@vue/eslint-config-airbnb": "^4.0.0",
    "axios": "^0.18.1",
    "jso": "^4.1.1",
    "lodash": "^4.17.13",
    "markdown-loader": "^5.1.0",
    "marked": "^0.7.0",
    "sweetalert2": "^7.33.1",
    "uuid4": "^1.1.4",
    "vue": "^2.6.6",
    "vue-infinite-loading": "^2.4.3",
    "vue-material": "0.8.1",
    "vue-router": "^3.0.2"
  },
  "devDependencies": {
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@vue/cli-plugin-babel": "^3.4.0",
    "@vue/cli-plugin-eslint": "^3.4.0",
    "@vue/cli-service": "^3.4.0",
    "eslint": "^5.13.0",
    "eslint-plugin-vue": "^5.1.0",
    "eslint-plugin-json": "^1.4.0",
    "html-loader": "^0.5.5",
    "jsdoc": "^3.6.3",
    "vue-template-compiler": "^2.6.6"
  },
  "babel": {
    "presets": [
      "@vue/app"
    ]
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "@vue/airbnb"
    ],
    "plugins": [
      "json"
    ],
    "rules": {
      "no-console": 0,
      "max-len": [
        "error",
        {
          "code": 140,
          "ignoreStrings": true
        }
      ]
    },
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
